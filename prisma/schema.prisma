  remarks         String?
  issue           String?
  deadline        DateTime?
  completed_by    String?
  task_uid        String
  task_order      Int       @db.SmallInt
  task_type       TaskType? @default(choice)
  list_choice     String[]  @default([])
  task_number_val Int?      @db.SmallInt
  task_check      Boolean?
  task_selected   String[]
  task_bool       Boolean?  @default(false)
